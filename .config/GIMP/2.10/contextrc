# GIMP user context

(tool "gimp-paintbrush-tool")
(paint-info "gimp-paintbrush")
(foreground (color-rgb 0 0 1))
(brush "2. Hardness 075")
(dynamics "Dynamics Off")
(mybrush "100% Opaque")
(pattern "Pine")
(gradient "FG to BG (RGB)")
(palette "Color History")
(font "Voynich V101")
(tool-preset "Core Pointer")

# end of user context
